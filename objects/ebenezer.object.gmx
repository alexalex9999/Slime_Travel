global.textboxarrayNPC[23] = ebenezer_neutral;

global.displaytextNPC[24] = "Now look at me. See the color of my slime? Lavender. LAVENDER.";
global.speakingNPC[24] = "ebenezer";
global.textboxarrayNPC[24] = ebenezer_angry;

global.displaytextNPC[25] = "Since the dawn of slime, purple has always been the most oppressed hue.";
global.speakingNPC[25] = "ebenezer";
global.textboxarrayNPC[25] = ebenezer_angry;

global.displaytextNPC[26] = "Condemned to peasantry, we have lived in the shadows of the warm.";
global.speakingNPC[26] = "ebenezer";
global.textboxarrayNPC[26] = ebenezer_neutral;

global.displaytextNPC[27] = "During the Great Slime Wars, we cool colors revolted.";
global.speakingNPC[27] = "ebenezer";
global.textboxarrayNPC[27] = ebenezer_neutral;

global.displaytextNPC[28] = "Taking up arms, we marched against the deposit slime king.";
global.speakingNPC[28] = "ebenezer";
global.textboxarrayNPC[28] = ebenezer_neutral;

global.displaytextNPC[29] = "We were crushed. Though at first he gave no heed, they destroyed us.";
