global.textboxarrayNPC[37] = slime_indifferent;

global.displaytextNPC[38] = "You still dont understand. Fine then. Let us go to the past.";
global.speakingNPC[38] = "ebenezer";
global.textboxarrayNPC[38] = ebenezer_neutral;

global.displaytextNPC[39] = "I will take you there. To the day of rebellion. The dawn of the war.";
