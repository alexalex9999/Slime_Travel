global.textboxarrayNPC[29] = ebenezer_angry;

global.displaytextNPC[30] = "We never stood a chance. It was then, that the Purple Purge began.";
global.speakingNPC[30] = "ebenezer";
global.textboxarrayNPC[30] = ebenezer_angry;

global.displaytextNPC[31] = "Suspicious of any peasants that might again rebel,";
global.speakingNPC[31] = "ebenezer";
global.textboxarrayNPC[31] = ebenezer_angry;

global.displaytextNPC[32] = "the warm aristocracy harshly persecuted the lower, cool slimes.";
global.speakingNPC[32] = "ebenezer";
global.textboxarrayNPC[32] = ebenezer_angry;

global.displaytextNPC[33] = "We tried and executed en masse. They suppressed information around it.";
global.speakingNPC[33] = "ebenezer";
global.textboxarrayNPC[33] = ebenezer_angry;

global.displaytextNPC[34] = "Even today, we will never have equality. Slime can never be equal.";
global.speakingNPC[34] = "ebenezer";
global.textboxarrayNPC[34] = ebenezer_sad;

global.displaytextNPC[35] = "I came to the past to right these wrongs, to correct these flaws.";
global.speakingNPC[35] = "ebenezer";
global.textboxarrayNPC[35] = ebenezer_neutral;

global.displaytextNPC[36] = "Youre stupid! Phil says you couldve screwed up everything!";
