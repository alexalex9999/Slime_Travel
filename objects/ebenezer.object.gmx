global.textboxarrayNPC[20] = ebenezer_sad;

global.displaytextNPC[21] = "But, the effects linger. Slimeciety has always been divided by color,";
global.speakingNPC[21] = "ebenezer";
global.textboxarrayNPC[21] = ebenezer_neutral;

global.displaytextNPC[22] = "(though we pretend this is not that case today.)";
